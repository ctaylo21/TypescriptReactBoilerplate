{
  "name": "typescriptreactboilerplate",
  "version": "3.1.0",
  "description": "Typescript & React boilerplate with Rollup, Eslint, and Jest",
  "repository": {
    "type": "git",
    "url": "https://github.com/ctaylo21/TypescriptReactBoilerplate.git"
  },
  "keywords": [
    "typescript",
    "react",
    "jest",
    "rollupjs",
    "eslint"
  ],
  "author": "Caleb Taylor <caleb89taylor@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "eslint --fix"
    ]
  },
  "scripts": {
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "start": "rollup -c rollup.dev.config.js -w",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rollup -c",
    "build:demo": "rollup -c rollup.dev.config.js --configDocBuild",
    "cm": "git-cz",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-url": "^5.0.1",
    "@svgr/rollup": "^5.4.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "commitizen": "^4.1.2",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-css-modules": "^2.1.0",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.13.1",
    "rollup": "^2.17.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-serve": "^1.0.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^26.1.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}